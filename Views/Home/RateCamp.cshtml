@{
    ViewData["Title"] = @ViewBag.ThisCamp.Name + " - RateMyCodingBootcamp - Rate a Bootcamp";
}
@model Review;

<!DOCTYPE html>
<html data-wf-page="60dce18002de9b3308838593" data-wf-site="60dbadc9c7028f161b9589a9">

<head>
    <meta charset="utf-8">
    <title>Rate</title>
    <meta content="Rate" property="og:title">
    <meta content="Rate" property="twitter:title">
    <meta content="width=device-width, initial-scale=1" name="viewport">
    <meta content="Webflow" name="generator">
    <link href="~/css/normalize.css" rel="stylesheet" type="text/css">
    <link href="~/css/webflow.css" rel="stylesheet" type="text/css">
    <link href="~/css/noahs-rmbc-practice.webflow.css" rel="stylesheet" type="text/css">
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js" type="text/javascript"></script>
    <script type="text/javascript">WebFont.load({ google: { families: ["Lora:regular,italic,700", "Oxygen:300,regular,700"] } });</script>
    <!-- [if lt IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv.min.js" type="text/javascript"></script><![endif] -->
    <script type="text/javascript">!function (o, c) { var n = c.documentElement, t = " w-mod-"; n.className += t + "js", ("ontouchstart" in o || o.DocumentTouch && c instanceof DocumentTouch) && (n.className += t + "touch") }(window, document);</script>
    <link href="~/images/favicon.ico" rel="shortcut icon" type="image/x-icon">
    <link href="~/images/webclip.png" rel="apple-touch-icon">
</head>

<body>
    <div>
        <div data-collapse="medium" data-animation="default" data-duration="400" style="opacity:0" role="banner"
            class="navbar w-hidden-main w-hidden-medium w-hidden-small w-hidden-tiny w-nav">
            <div class="w-container">
                <a href="#" class="logo w-nav-brand"></a>
                <nav role="navigation" class="w-nav-menu">
                    <a href="/" class="w-nav-link">Home</a>
                    <a href="/about" class="w-nav-link">About</a>
                    <a href="#" class="w-nav-link">Contact</a>
                </nav>
                <div class="w-nav-button">
                    <div class="w-icon-nav-menu"></div>
                </div>
            </div>
        </div>
        <div class="inner-hero about-page">
            <div class="w-container">
                <h1 class="hero-bootcamp"><strong>RateMyCodingBootcamp</strong></h1>
            </div>
        </div>
        <div class="section-3 w-clearfix">
            <a href="/" class="link-block w-inline-block"></a>
            <h1 class="heading-3">Rate @ViewBag.ThisCamp.Name </h1>
            <a href="/bootcamp/id" class="link-4">See all @ViewBag.ThisCamp.Name reviews</a>
        </div>
        <div id="posts" class="main-section">
            <div class="columns-2 w-row">
                <div class="w-col w-col-2 w-col-small-3 w-col-tiny-3"></div>
                <div class="column-3 w-col w-col-4 w-col-small-3 w-col-tiny-3">
                    <h3 class="heading-top">What is your Cohort ID or Student ID?</h3>
                    <p class="paragraph-2">This will not be public. Your review may be subject to removal if you do not include this verification.</p>
                    <h3 class="heading-top">What course did you take?</h3>
                    <p class="paragraph-3">If your course is not listed, please select the closest option.</p>
                    <h2 class="heading-4">What location did you attend?</h2>
                    <h2 class="heading-5">Rate the curriculum out of 5 stars</h2>
                    <p class="paragraph-4">Keep in mind: coursework, assignments, exams</p>
                    <h2 class="heading-6">Rate the difficulty out of 5 stars</h2>
                    <p class="paragraph-5">Keep in mind: cirriculum, assistance accessibility, concept disassembly</p>
                    <h2 class="heading-7">Rate the instructors out of 5 stars</h2>
                    <p class="paragraph-6">Keep in mind: interactions, 1 on 1 time, learning experience</p>
                    <h2 class="heading-8">Rate career support out of 5 stars</h2>
                    <p class="paragraph-7">Keep in mind: career services offered, coaching offered, resume/portfolio assistance</p>
                    <h2 class="heading-9">Rate the facility out of 5 stars</h2>
                    <p class="paragraph-8">Keep in mind: learning environment, equipment - or select online for virtual</p>
                    <h2 class="heading-10">Was this a online course?</h2>
                    <h2 class="heading-11">Was textbook use part of the curriculum?</h2>
                    <h2 class="heading-12">What were some pros for you?</h2>
                    <p class="paragraph-10">Brief list of the pros in your eyes. Keep it short and sweet, under 100 characters</p>
                    <h2 class="heading-13">What were some cons for you?</h2>
                    <p class="paragraph-11">Brief list of the cons in your eyes. Keep it short and sweet, under 100 characters</p>
                    <h2 class="heading-14">Would you recommend this bootcamp?</h2>
                    <p class="paragraph-9">To a friend, or colleague</p>
                    <h2 class="heading-15">Write a comment</h2>
                    <p class="paragraph-12">Here&#x27;s your chance to be specific! Let others know how you feel &amp; what to expect!</p>
                </div>
                <div class="w-col w-col-3 w-col-small-3 w-col-tiny-3">
                    <div class="w-form">
                        <form action="/rate/submit" id="email-form" name="email-form" data-name="Form">
                            
                            <input type="text" class="text-field w-input" maxlength="256" asp-for="Cohort" data-name="Cohort" placeholder="Cohort ID or Student ID" id="Corhort" required="">
                            <select id="Course" asp-for="Course" required="" data-name="Course" class="select-field w-select">
                                @foreach (var i in ViewBag.ThisCamp.Courses) {<option value="@i.Name">@i.Name</option>}
                                
                            </select><select id="field-2" asp-for="Location" required="" data-name="Location"
                                class="select-field-2 w-select">
                                <option value="Online">Online / Virtual</option>
                                @foreach (var i in ViewBag.ThisCamp.Locations) 
                                {<option value="@i.State.Name">@i.State.Name</option>}
                                
                            </select><select id="field-3" asp-for="Curriculum" required="" class="select-field-3 w-select">
                                <option disabled selected>Select from menu</option>
                                <option value="1">One - Terrible</option>
                                <option value="2">Two - Poor</option>
                                <option value="3">Three - Average/Acceptable</option>
                                <option value="4">Four - Good</option>
                                <option value="5">Five - Exellent</option>
                                
                            </select><select id="field-4" asp-for="Difficulty" required="" class="select-field-4 w-select">
                                <option disabled selected>Select from menu</option>
                                <option value="1">VERY Easy / Too Easy</option>
                                <option value="2">Easy</option>
                                <option value="3">Average / Medium</option>
                                <option value="4">Difficult</option>
                                <option value="5">VERY Difficult / Too Difficult</option>
                                
                            </select><select id="field-5" asp-for="Instructors" class="select-field-5 w-select">
                                <option disabled selected>Select from menu</option>
                                <option value="1">One - Terrible</option>
                                <option value="2">Two - Poor</option>
                                <option value="3">Three - Average/Acceptable</option>
                                <option value="4">Four - Good</option>
                                <option value="5">Five - Exellent</option>
                                
                            </select><select id="field-6" asp-for="JobSupport" class="select-field-6 w-select">
                                <option disabled selected>Select from menu</option>
                                <option value="1">One - Terrible</option>
                                <option value="2">Two - Poor</option>
                                <option value="3">Three - Average/Acceptable</option>
                                <option value="4">Four - Good</option>
                                <option value="5">Five - Exellent</option>
                                
                            </select><select id="field-7" asp-for="Facility" class="select-field-7 w-select">
                                <option disabled selected>Select from menu</option>
                                <option value="5">N/A - Online</option>
                                <option value="1">One - Terrible</option>
                                <option value="2">Two - Poor</option>
                                <option value="3">Three - Average/Acceptable</option>
                                <option value="4">Four - Good</option>
                                <option value="5">Five - Exellent</option>
                                
                            </select><label class="w-checkbox checkbox-field">
                                <input type="checkbox" id="Virtual" asp-for="Online" data-name="Online" class="w-checkbox-input">
                                <span for="Online" class="w-form-label">Yes, this was a virtual course</span></label>
                                
                            <select id="field-8" asp-for="Textbook" class="select-field-9 w-select">
                                <option disabled selected>Select from menu</option>
                                <option value="Optional">Avalible but Optional</option>
                                <option value="Always">Always / Daily</option>
                                <option value="Often">Often</option>
                                <option value="Sometimes">Sometimes</option>
                                <option value="Rarely">Rarely</option>
                                <option value="Never">Never</option>
                                
                            </select>
                            
                            <input type="text" class="text-field-2 w-input" maxlength="256" asp-for="Pros" data-name="Pros" placeholder="Pros" id="Pros" required="">
                            
                            <input type="text" class="text-field-3 w-input" maxlength="256" asp-for="Cons" data-name="Cons" placeholder="Cons" id="Cons" required="">
                            
                            <select id="Reccomend" asp-for="Recommend" data-name="Reccomend"
                                class="select-field-8 w-select">
                                <option value="YES">Yes!</option>
                                <option value="NO">No...</option>
                                
                            </select><textarea placeholder="Be specific here! Max 350 characters" maxlength="5000" id="field-9" asp-for="Comments" class="textarea w-input"></textarea>
                            
                            <input type="hidden" asp-for="BootcampID" value="@ViewBag.ThisCamp.BootcampID">
                            
                            <input type="submit" value="Submit" data-wait="Please wait..." class="w-button">
                            
                        </form>
                        <div class="w-form-done">
                            <div>Thank you! Your submission has been received!</div>
                        </div>
                        <div class="w-form-fail">
                            <div>Oops! Something went wrong while submitting the form.</div>
                        </div>
                    </div>
                </div>
                <div class="w-col w-col-3 w-col-small-3 w-col-tiny-3"></div>
            </div>
            <p class="paragraph-13">Your ratings and comments are subject to review when submitted. Please be honest &amp; respectful.</p>
        </div>
        <div class="main-section dark">
            <div class="footer-section">
                <div class="w-container">
                    <div class="w-row">
                        <div class="w-col w-col-4 w-col-small-4">
                            <a href="/" class="footer-logo w-inline-block">
                                <div class="long-text">RateMyCodingBootcamp © 2021</div>
                            </a>
                        </div>
                        <div class="footer-links w-col w-col-8 w-col-small-8">
                            <a href="/" class="footer-link">Home</a>
                            <a href="/rate" class="footer-link">Write a Review </a>
                            <a href="/about" class="footer-link">About</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=60dbadc9c7028f161b9589a9"
        type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <script src="~/js/webflow.js" type="text/javascript"></script>
    <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->
</body>

</html>